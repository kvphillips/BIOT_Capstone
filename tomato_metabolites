import requests
import time

kegg_api_url = "http://rest.kegg.jp/find/compound/tomato"

response = requests.get(kegg_api_url)

compound_data = response.text

print(compound_data)

tomato_compound_ids = []

for line in compound_data.split('\n'):
    parts = line.split()
    if len(parts) > 0:
        compound_id = parts[0]
        tomato_compound_ids.append(compound_id)

for compound_id in tomato_compound_ids:
    print("Compound ID:", compound_id)

time.sleep(2)

